                "string_fields" : {
                    "mapping" : {
                        "index" : "not_analyzed",
                        "omit_norms" : true,
                        "type" : "string"
                    },
                    "match" : "*",
                    "match_mapping_type" : "string"
                }
            }, {
                "float_fields" : {
                    "mapping" : {
                        "type" : "float",
                        "doc_values" : true
                    },
                    "match" : "*",
                    "match_mapping_type" : "double"
                }
            }, {
