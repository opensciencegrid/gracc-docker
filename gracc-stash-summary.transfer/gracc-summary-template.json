{
    "index_patterns" : "gracc.*.summary3-*",
    "settings" : {
        "index" : {
            "translog" : {
                "sync_interval" : "5s",
                "durability" : "async"
            },
            "number_of_shards" : 3,
            "number_of_replicas" : 1,
            "refresh_interval" : "60s"
        }
    },
    "mappings" : {
